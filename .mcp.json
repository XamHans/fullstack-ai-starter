{
  "mcpServers": {
    "next-devtools": {
      "command": "npx",
      "args": ["-y", "next-devtools-mcp@latest"]
    },
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        " --from git+https://github.com/oraios/serena serena start-mcp-server"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "--package",
        "mollie-api-typescript",
        "--",
        "mcp",
        "start",
        "--api-key",
        "...",
        "--o-auth",
        "...",
        "--profile-id",
        "...",
        "--testmode",
        "...",
        "--custom-user-agent",
        "..."
      ]
    },
    "mollie": {
      "command": "npx",
      "args": [
        "-y",
        "--package",
        "mollie-api-typescript",
        "--",
        "mcp",
        "start",
        "--api-key",
        "...",
        "--o-auth",
        "...",
        "--profile-id",
        "...",
        "--testmode",
        "...",
        "--custom-user-agent",
        "..."
      ]
    },
    "neon": {
      "command": "npx",
      "args": ["-y", "mcp-remote@latest", "https://mcp.neon.tech/mcp"]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp@latest"]
    }
  }
}
